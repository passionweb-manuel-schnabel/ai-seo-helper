{
  "name": "passionweb/ai-seo-helper",
  "license": "GPL-3.0",
  "description": "Generates SEO metadata based on content using AI. Currently several metadata for pages and articles of EXT:news can be generated using an additional button next to the corresponding input fields.",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "0.7.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^11.5 | ^12.4 | ^13.4",
    "typo3/cms-backend": "^11.5 | ^12.4 | ^13.4"
  },
  "conflict": {
    "autodudes/ai-suite": "*"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\AiSeoHelper\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/ai-seo-helper": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ai_seo_helper"
    }
  }
}
